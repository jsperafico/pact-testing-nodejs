{
  "consumer": {
    "name": "app-notify-client"
  },
  "provider": {
    "name": "app-client-data"
  },
  "interactions": [
    {
      "description": "a request to get a client",
      "providerState": "a client with name 'edina' exists",
      "request": {
        "method": "GET",
        "path": "/client-data/edina",
        "headers": {
          "Authorization": "Bearer Some_valid_Token"
        },
        "matchingRules": {
          "$.headers.Authorization": {
            "match": "type"
          }
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json; charset=utf-8"
        },
        "body": {
          "name": "Jena M. Medina",
          "email": "Morbi.quis.urna@tortorIntegeraliquam.org",
          "phone": "(914) 842-4818"
        },
        "matchingRules": {
          "$.headers.Content-Type": {
            "match": "regex",
            "regex": "application\\/json;?.*"
          },
          "$.body": {
            "match": "type"
          }
        }
      }
    },
    {
      "description": "a request to get a client",
      "providerState": "a client with name 'hello' dosn't exists",
      "request": {
        "method": "GET",
        "path": "/client-data/hello",
        "headers": {
          "Authorization": "Bearer Some_valid_Token"
        },
        "matchingRules": {
          "$.headers.Authorization": {
            "match": "type"
          }
        }
      },
      "response": {
        "status": 404,
        "headers": {
        }
      }
    },
    {
      "description": "a request to get a client",
      "providerState": "Authorization token is invalid",
      "request": {
        "method": "GET",
        "path": "/client-data/hello",
        "headers": {
          "Authorization": "Bearer Some_Invalid_Token"
        },
        "matchingRules": {
          "$.path": {
            "match": "type"
          }
        }
      },
      "response": {
        "status": 401,
        "headers": {
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}